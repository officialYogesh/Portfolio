# Environment Variables for Portfolio Website
# Copy this file to .env.local and update with your actual values

# Site Configuration
NEXT_PUBLIC_SITE_URL=http://localhost:3000
NEXT_PUBLIC_SITE_NAME="Yogesh Patil Portfolio"

# Analytics (Optional)
# NEXT_PUBLIC_GA_ID=your_google_analytics_id
# NEXT_PUBLIC_GTM_ID=your_google_tag_manager_id

# Contact Form (Vercel Forms will be configured later)
# NEXT_PUBLIC_FORM_API_URL=your_form_api_url

# Theme Configuration
NEXT_PUBLIC_DEFAULT_THEME="dracula"

# Development
NODE_ENV=development 